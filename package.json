{
    "name": "vscode-faust",
    "displayName": "Faust",
    "description": "Language support for the Faust DSP programming language",
    "version": "1.1.0",
    "publisher": "glen-anderson",
    "repository": {
        "url": "https://github.com/hellbent/vscode-faust.git"
    },
    "homepage": "https://github.com/hellbent/vscode-faust/blob/master/README.md",
    "icon": "images/faust-icon.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "faust",
                "aliases": [
                    "Faust",
                    "faust"
                ],
                "extensions": [
                    ".dsp",
                    ".lib"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "faust",
                "scopeName": "source.faust",
                "path": "./syntaxes/faust.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "faust",
                "path": "./snippets/faust-snippets.json"
            }
        ]
    }
}
