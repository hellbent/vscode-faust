{
    "name": "vscode-faust",
    "displayName": "vscode-faust",
    "description": "Language support for the Faust DSP programming language",
    "version": "1.0.0",
    "publisher": "Glen Anderson",
    "repository": "hellbent/vscode-faust",
    "homepage": "https://github.com/hellbent/vscode-faust",
    "license": "MIT",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "faust",
            "aliases": ["Faust", "faust"],
            "extensions": [".dsp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "faust",
            "scopeName": "source.faust",
            "path": "./syntaxes/faust.tmLanguage.json"
        }]
    }
}
